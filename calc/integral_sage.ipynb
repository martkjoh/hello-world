   "source": [
    "from IPython.display import display, Latex\n",
    "\n",
    "def pprint(x, name=\"\"):\n",
    "    txt = \"$\"\n",
    "    if name != \"\": txt += name\n",
    "    txt += latex(x) + \"$\"\n",
    "    return Latex(txt)\n",
    "\n",
    "\n",
    "var(\"m\")\n",
    "assume(m>0)\n",
    "f(x) = 1 / sqrt(-(1 - 2*m / x))\n",
    "pprint(f(x))"
   ]
  },
